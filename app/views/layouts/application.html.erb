<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <%= csrf_meta_tag%>
    <%= render :partial => "layouts/stylesheets" %>
  </head>
  <body>
    <div class="container">
      <%= render :partial => "layouts/header" %>
      <section class="round">
        <p class="notice"><%= notice %></p>
        <p class="alert"><%= alert %></p>
        <%= yield %>
      </section>
      <%= render 'layouts/footer' %>
     </div>
  </body>
</html>

